  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miggi92/odata-fw/issues"
  },
  "homepage": "https://github.com/miggi92/odata-fw/blob/master/README.md",
  "devDependencies": {
    "@abaplint/cli": "^2.113.204",
    "@abaplint/runtime": "^2.11.92",
    "@abaplint/transpiler": "^2.11.92",
    "@abaplint/transpiler-cli": "^2.11.92"
  },
  "packageManager": "pnpm@10.28.2"
}
